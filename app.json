{
  "expo": {
    "name": "seconds-driver",
    "owner": "seconds-technologies",
    "slug": "seconds-driver",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "seconds-technologies",
            "project": "seconds-driver",
            "authToken": "491de6635aa0474b8fb1a44d8589c62232d456c72b8240f889965d6a7fca6e2e"
          }
        }
      ]
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Seconds Driver to access your photos.",
          "savePhotosPermission": "Allow Seconds Driver to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Seconds Driver to access your photos."
        }
      ]
    ]
  }
}
