{
  "name": "seconds-driver",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start -p 19000",
    "start-clean": "expo start --clear",
    "start-dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web -p 19001",
    "expo-build-dev": "eas build --profile development",
    "expo-build-prod": "eas build --profile production",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "@react-native-async-storage/async-storage": "^1.16.0",
    "@react-native-picker/picker": "^2.2.1",
    "@react-navigation/drawer": "^6.3.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@reduxjs/toolkit": "^1.7.2",
    "@sentry/react-native": "3.3.5",
    "@sentry/tracing": "^6.19.2",
    "axios": "^0.25.0",
    "classnames": "^2.3.1",
    "expo": "^44.0.6",
    "expo-application": "~4.0.1",
    "expo-blur": "^11.0.0",
    "expo-camera": "^12.1.2",
    "expo-constants": "~13.0.1",
    "expo-device": "~4.1.0",
    "expo-file-system": "~13.1.4",
    "expo-location": "^14.0.2",
    "expo-media-library": "~14.0.0",
    "expo-notifications": "~0.14.0",
    "expo-secure-store": "~11.1.0",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "expo-task-manager": "~10.1.0",
    "expo-updates": "~0.11.6",
    "formik": "^2.2.9",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "2.1.1",
    "react-native-logs": "^4.0.1",
    "react-native-modal": "^13.0.0",
    "react-native-pager-view": "5.4.9",
    "react-native-reanimated": "^2.4.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.2",
    "react-native-signature-canvas": "^4.3.0",
    "react-native-svg": "^12.1.1",
    "react-native-switch-toggle": "^2.2.1",
    "react-native-tab-view": "^3.1.1",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-user-avatar": "^1.0.8",
    "react-native-web": "0.17.1",
    "react-native-webview": "11.15.0",
    "react-redux": "^7.2.6",
    "react-use": "^17.3.2",
    "redux-persist": "^6.0.0",
    "sentry-expo": "^4.0.0",
    "tailwind-rn": "^4.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react-redux": "^7.1.22",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "react-native-dotenv": "^3.3.1",
    "redux-logger": "^3.0.6",
    "tailwindcss": "^3.0.19"
  },
  "private": true,
  "prettier": {
    "arrowParens": "avoid",
    "useTabs": true,
    "tabWidth": 4,
    "printWidth": 150,
    "trailingComma": "none",
    "jsxSingleQuote": true,
    "semi": true,
    "bracketSpacing": true,
    "react/jsx-max-props-per-line": [
      1,
      {
        "when": "always"
      }
    ]
  }
}
