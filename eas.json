{
  "build": {
    "production": {
      "releaseChannel": "production",
      "env": {
        "API_BASE_URL": "https://seconds-api.herokuapp.com",
        "SERVER_BASE_URL": "https://seconds-app-server.herokuapp.com",
        "ENV_MODE": "production"
      }
    },
    "staging": {
      "releaseChannel": "staging",
      "distribution": "internal",
      "env": {
        "API_BASE_URL": "https://seconds-api-dev.herokuapp.com",
        "SERVER_BASE_URL": "https://seconds-app-server-dev.herokuapp.com",
        "ENV_MODE": "staging"
      }
    },
    "development": {
      "releaseChannel": "dev",
      "distribution": "internal",
      "env": {
        "API_BASE_URL": "https://seconds-api-dev.herokuapp.com",
        "SERVER_BASE_URL": "https://seconds-app-server-dev.herokuapp.com",
        "ENV_MODE": "dev"
      }
    }
  },
  "cli": {
    "version": ">= 0.47.0",
    "requireCommit": true
  }
}
